<header>
    <div class="app-wrapper nav">
        <div class="nav-left">
            <div class="logo">
                {{{ getIcon "nav" "logo" }}}
            </div>
        </div>

        <div id="navRight" class="nav-right">
            <ul class="social-links">
                {{#each data.socialLinks}}
                <li>
                    <a href="{{this.url}}" target="_black">
                        <img src="images/nav/{{this.name}}.svg" alt="{{this.name}}">
                    </a>
                </li>
                {{/each}}
            </ul>
            <button id="menuButton" class="menu-button">=</button>
            <div id="menuPane" class="menu-pane">
                {{#each data.menuItems}}
                <label>
                    <input type="radio" name="nav-button" />
                    <button class="button" style="pointer-events: none">
                        {{this.name}}
                    </button>
                </label>
                {{/each}}
            </div>
        </div>
    </div>
</header>